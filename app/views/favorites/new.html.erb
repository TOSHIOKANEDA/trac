<% content_for :stylesheets do %>
  <%= stylesheet_link_tag "favorites/new", "data-turbo-track": "reload" %>
  <%= stylesheet_link_tag "commons/port", "data-turbo-track": "reload" %>
<% end %>

<% content_for :title, "見積り編集" %>

<div data-controller="favorite-new" style="width:100%;">
  <%= render partial: "layouts/sidebar" %>
  <div class="main-content">
    <div class="container">
      <%= form_with model: @quotation, url: quotations_path, method: :post, local: true, data: { turbo: false } do |f| %>
        <%= render "form", f: f %>
      <% end %>
    </div>   
  </div>
</div>
